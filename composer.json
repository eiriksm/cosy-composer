{
    "name": "eiriksm/cosy-composer",
    "description": "I will probably use this for something",
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.2",
        "satooshi/php-coveralls": "^2.0"
    },
    "require": {
        "composer/composer": "^1.4",
        "violinist-dev/symfony-cloud-security-checker": "dev-master",
        "knplabs/github-api": "^2.0",
        "php-http/guzzle6-adapter": "^1.1",
        "violinist-dev/git-log-format": "dev-master",
        "violinist-dev/violinist-messages": "^1",
        "violinist-dev/slug-from-url": "dev-master",
        "wa72/simplelogger": "^1.0",
        "league/flysystem": "^1.0",
        "eiriksm/symfony-console-array-output": "^1.0",
        "violinist-dev/project-data": "dev-master",
        "m4tthumphrey/php-gitlab-api": "^9.10",
        "peterpostmann/parse_uri": "^1.0"
    },
    "autoload": {
        "psr-4": {"eiriksm\\CosyComposer\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "eiriksm\\CosyComposerTest\\": "test"
        }
    },
    "scripts": {
        "test": "@composer lint && phpunit",
        "lint": "phpcs -p -n"
    }
}
